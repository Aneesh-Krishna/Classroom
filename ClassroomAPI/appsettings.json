{
  "AWS": {
    "Profile": "your_profile",
    "Region": "your_region", // Change to your preferred region 
    "AccessKey": "your_accesskey",
    "SecretKey": "your_secretkey",
    "S3BucketName": "your_bucketname"
  },
  "ConnectionStrings": {
    "DefaultConnection": "your_connectionstring"
  },
  "Jwt": {
    "Key": "your_jwtkey",
    "Issuer": "your_issuer",
    "Audience": "your_audience"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
